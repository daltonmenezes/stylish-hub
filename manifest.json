{
  "name": "stylish-hub",
  "description": "A browser extension that brings a new GitHub experience",
  "version": "1.0.0",
  "homepage_url": "https://github.com/daltonmenezes/stylish-hub",
  "manifest_version": 2,
  "content_scripts": [{
    "matches": [
      "*://github.com/*",
      "*://*.github.com/*",
      "*://www.slideshare.net/*"
    ],
    "css": ["entry.css"],
    "js": ["entry.js"],
    "run_at": "document_idle"
  }],
  "permissions": [
    "*://github.com/*",
    "*://*.github.com/*",
    "*://www.slideshare.net/*"
  ],
  "icons": {
    "48": "icons/48x.png",
    "64": "icons/64x.png",
    "96": "icons/96x.png",
   "128": "icons/128x.png"
  }
}
